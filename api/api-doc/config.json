"/v2/config/{id}": {
      "get": {
        "description": "Return a config value",
        "operationId": "find_config_id",
        "produces": [
          "application/json"
        ],
        "tags": ["config"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of config to return",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Config value"
          },
          "default": {
            "description": "unexpected error",
            "schema": {
              "$ref": "#/definitions/ErrorModel"
            }
          }
        }
      }
},
"/v2/config/object_store_endpoints": {
      "get": {
        "description": "Returns the list of configured object storage endpoints",
        "operationId": "get_object_store_endpoints",
        "produces": [
          "application/json"
        ],
        "tags": ["config"],
        "parameters": [ ],
        "responses": {
          "200": {
            "schema": {
              "type":"array",
              "items":{
                  "$ref":"#/definitions/object_store_endpoint",
                  "description":"object store endpoint"
              }
            },
            "description": "the list of configured object storage endpoints"
          },
          "default": {
            "description": "unexpected error",
            "schema": {
              "$ref": "#/definitions/ErrorModel"
            }
          }
        }
      }
}
